RewriteEngine on
#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule .* index.php/$0 [PT,L]

RewriteCond $1 ^(.+\.css)
RewriteRule ^(.*)$ $1 [L]

RewriteCond $1 ^(.+\.js)
RewriteRule ^(.*)$ $1 [L]

RewriteCond $1 ^(.+\.ttf)
RewriteRule ^(.*)$ $1 [L]

RewriteCond $1 ^(.+\.woff)
RewriteRule ^(.*)$ $1 [L]



